{
   "cli": {
      "version": ">= 16.17.4",
      "appVersionSource": "remote"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "android": {
            "buildType": "apk"
         },
         "env": {
            "SENTRY_DISABLE_AUTO_UPLOAD": "true"
         }
      },
      "preview": {
         "distribution": "internal",
         "android": {
            "buildType": "apk"
         },
         "developmentClient": true,

         "env": {
            "SENTRY_DISABLE_AUTO_UPLOAD": "true"
         }
      },
      "production": {
         "autoIncrement": true,

         "env": {
            "SENTRY_DISABLE_AUTO_UPLOAD": "true",
            "DATABASE_URL": "@DATABASE_URL",
            "JWT_SECRET": "@JWT_SECRET",
            "JWT_REFRESH_SECRET": "@JWT_REFRESH_SECRET",
            "GOOGLE_CLIENT_ID": "@GOOGLE_CLIENT_ID",
            "GOOGLE_CLIENT_SECRET": "@GOOGLE_CLIENT_SECRET",
            "OPENAI_API_KEY": "@OPENAI_API_KEY",
            "R2_ACCOUNT_ID": "@R2_ACCOUNT_ID",
            "R2_ACCESS_KEY_ID": "@R2_ACCESS_KEY_ID",
            "R2_SECRET_ACCESS_KEY": "@R2_SECRET_ACCESS_KEY",
            "R2_BUCKET_NAME": "@R2_BUCKET_NAME",
            "R2_ENDPOINT": "@R2_ENDPOINT",
            "CLOUDFLARE_R2_PUBLIC_URL": "@CLOUDFLARE_R2_PUBLIC_URL",
            "SENTRY_AUTH_TOKEN": "@SENTRY_AUTH_TOKEN",
            "EXPO_PUBLIC_BASE_URL": "@EXPO_PUBLIC_BASE_URL",
            "EXPO_PUBLIC_SCHEME": "@EXPO_PUBLIC_SCHEME",
            "EXPO_PUBLIC_MIXPANEL_TOKEN": "@EXPO_PUBLIC_MIXPANEL_TOKEN",
            "EXPO_PUBLIC_API_SECRET": "@EXPO_PUBLIC_API_SECRET",
            "EXPO_PUBLIC_MIXPANEL_SERVER_URL": "@EXPO_PUBLIC_MIXPANEL_SERVER_URL",
            "APPLE_CLIENT_SECRET": "@APPLE_CLIENT_SECRET",
            "APPLE_CLIENT_ID": "@APPLE_CLIENT_ID",
            "APPLE_REDIRECT_URI": "@APPLE_REDIRECT_URI",
            "APPLE_AUTH_URL": "@APPLE_AUTH_URL"
         }
      }
   },
   "submit": {
      "production": {}
   }
}
