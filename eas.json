{
   "cli": {
      "version": ">= 16.17.4",
      "appVersionSource": "remote"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "android": {
            "buildType": "apk"
         },
         "env": {
            "SENTRY_DISABLE_AUTO_UPLOAD": "true"
         }
      },
      "preview": {
         "distribution": "internal",
         "android": {
            "buildType": "apk"
         },
         "env": {}
      },
      "production": {
         "autoIncrement": true
      }
   },
   "submit": {
      "production": {}
   }
}
